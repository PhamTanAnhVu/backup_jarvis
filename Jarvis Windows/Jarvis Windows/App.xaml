<Application x:Class="Jarvis_Windows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Jarvis_Windows"
             xmlns:mainView="clr-namespace:Jarvis_Windows.Sources.MVVM.Views.MainView"
             >
    <Application.Resources>
        <DataTemplate DataType="{x:Type TypeName=mainView:MainViewModel}">
            <mainView:MainView/>
        </DataTemplate>


        <BooleanToVisibilityConverter x:Key="VisibilityConverter" />
        
    </Application.Resources>
</Application>
