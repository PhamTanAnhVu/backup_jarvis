<Application x:Class="Jarvis_Windows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Jarvis_Windows"
             xmlns:mainView="clr-namespace:Jarvis_Windows.Sources.MVVM.Views.MainView"
             xmlns:settingView="clr-namespace:Jarvis_Windows.Sources.MVVM.Views.SettingView">
    <Application.Resources>
        <ResourceDictionary>
            <BooleanToVisibilityConverter x:Key="VisibilityConverter" />
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Jarvis Windows;component/Sources/Utils/Core/DataTemplate.xaml"/>
                <ResourceDictionary Source="/Jarvis Windows;component/Sources/Utils/Core/Page.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
