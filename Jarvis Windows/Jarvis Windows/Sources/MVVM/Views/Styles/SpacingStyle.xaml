    <CornerRadius x:Key="BorderRadiusTop3Xl">24 24 0 0</CornerRadius>
    <CornerRadius x:Key="BorderRadiusTopFull">9999 9999 0 0</CornerRadius>

    <!--Right CornerRadius-->
    <CornerRadius x:Key="BorderRadiusRightSm">0 2 0 2</CornerRadius>
    <CornerRadius x:Key="BorderRadiusRight">0 4 0 4</CornerRadius>
