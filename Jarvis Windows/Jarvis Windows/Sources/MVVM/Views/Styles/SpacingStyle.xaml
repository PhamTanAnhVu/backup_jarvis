    <!-- CornerRadius Styles -->
    <!--Full CornerRadius-->
    <CornerRadius x:Key="BorderRadiusNone">0</CornerRadius>
    <CornerRadius x:Key="BorderRadiusSm">2</CornerRadius>
    <CornerRadius x:Key="BorderRadius">4</CornerRadius>
    <CornerRadius x:Key="BorderRadiusMd">6</CornerRadius>
    <CornerRadius x:Key="BorderRadiusLg">8</CornerRadius>
    <CornerRadius x:Key="BorderRadiusXl">12</CornerRadius>
    <CornerRadius x:Key="BorderRadius2Xl">16</CornerRadius>
    <CornerRadius x:Key="BorderRadius3Xl">24</CornerRadius>
    <CornerRadius x:Key="BorderRadiusFull">9999</CornerRadius>
    
    <!--Left CornerRadius-->
    <CornerRadius x:Key="BorderRadiusLeftSm">2 0 0 2</CornerRadius>
    <CornerRadius x:Key="BorderRadiusLeft">4 0 0 4</CornerRadius>
