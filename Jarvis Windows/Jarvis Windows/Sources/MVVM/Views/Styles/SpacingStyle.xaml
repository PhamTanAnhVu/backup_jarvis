    <CornerRadius x:Key="BorderRadiusLeftFull">9999 0 0 9999</CornerRadius>

    <!--Top CornerRadius-->
    <CornerRadius x:Key="BorderRadiusTopSm">2 2 0 0</CornerRadius>
    <CornerRadius x:Key="BorderRadiusTop">4 4 0 0</CornerRadius>
