    <!--=====================================START Textblock============================================-->
    <!-- Header TextBlock Style -->
    <Style x:Key="BaseHeader_TextblockStyle" TargetType="TextBlock">
        <Setter Property="FontFamily" Value="Inter"/>
        <Setter Property="FontStyle" Value="Normal"/>
        <Setter Property="FontWeight" Value="Bold"/>
    </Style>

    <Style x:Key="Header_Textblock1" BasedOn="{StaticResource BaseHeader_TextblockStyle}" TargetType="TextBlock">
        <Setter Property="FontSize" Value="34"/>
        <Setter Property="LineHeight" Value="1.3"/>
    </Style>

    <Style x:Key="Header_Textblock2" BasedOn="{StaticResource BaseHeader_TextblockStyle}" TargetType="TextBlock">
